{
  "compilerOptions": {
    "jsx": "react-jsx", // ou "react" para vers√µes antigas
    "module": "ESNext",
    "target": "ES6",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
